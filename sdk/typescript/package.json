{
  "name": "@streamasr/openai-realtime-sdk",
  "version": "1.0.0",
  "description": "TypeScript client SDK for StreamASR OpenAI Realtime API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && npm run build:test",
    "build:test": "node scripts/build-test.js",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "serve:test": "node scripts/start-test-server.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "streamasr",
    "openai",
    "realtime",
    "asr",
    "speech-recognition",
    "websocket",
    "typescript",
    "sdk"
  ],
  "author": "StreamASR Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/streamasr/openai-realtime-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/streamasr/openai-realtime-sdk/issues"
  },
  "homepage": "https://docs.streamasr.com",
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
